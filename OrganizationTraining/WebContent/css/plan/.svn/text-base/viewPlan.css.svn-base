	display: none;
}

#publichContent {
	display: none;
}

#brief {
	display: block;
	overflow: auto;
}

.realNameSpan {
	display: none;
}

.nameDetail {
	width: 140px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}

#inputTipDiv {
	position: absolute;
	top: 80px;
	left: 30px;
	color: #BFBFBF;
}
<!-- apply leave confirm bar css -->
#leaveContent {
	width: 292px;
	text-align: left;
}

.inputTipDiv_leave {
	position: absolute;
	top: 10px;
	left: 7px;
	color: #BFBFBF;
}
.leaveReason {
	width: 290px; 
	height: 74px;
	margin-top: 8px;
	padding-top: 6px;
	padding-bottom: 6px;
}

.leave_name_span {
	font-weight: bold;
}

.common_checkbox{
    margin-right: 20px;
}

.planCourseTraner {
	width:90px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}

.realPlanCourseTranerSpan {
	display: none;
}