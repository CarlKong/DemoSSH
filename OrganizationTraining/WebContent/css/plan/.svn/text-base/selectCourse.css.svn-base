@CHARSET "UTF-8";

#greybackground {
    background:#000000 none repeat scroll 0 0;
    left:0;
    position:absolute;
    top:0;
    width:100%;
    z-index:100;
}

#selectCourseDiv {
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	z-index: 101;
}

#selectCourse {
	position: relative;
	width: 960px;
	height: 540px;
	margin: 0;
	padding: 0;
	margin: 0 auto;
	background-color: white;
	color: #555555;
}

.selectCourseTitle {
	float: left;
	background: #71B3D6;
	width: 960px;
	height: 30px;
}

#closePic {
	float: right;
	display: inline;
	padding-top: 10px;
	padding-right: 10px;
	cursor: pointer;
}

#topTitle {
	display: inline-block;
	float: left;
	height: 30px;
	line-height: 30px;
	text-align: center;
	color: #FFFFFF;
	padding-left: 10px;
}

/******* selected result part *******/
#selectedCourseLabel {
	position: absolute;
	bottom: 102px;
	left: 10px;
}

.resultLeft, .resultRight {
    width: 16px;
    height: 44px;
}

.resultLeftDisable, .resultLeft {
    width: 16px;
	height: 44px;
	display:inline-block;
    margin-left:10px;
	float: left;
}
	
.resultLeftDisable {
	background: url("../../image/plan/selectCourse/ICN_Previous_Disable_16x44.png") no-repeat;
}

.resultLeft {
    background: url("../../image/plan/selectCourse/ICN_Previous_16x44.png") no-repeat;
}

.resultRight, .resultRightDisable {
	width: 16px;
	height: 44px;
	display:inline-block;
    margin-right:10px;
	float: right;
}

.resultRightDisable {
    background: url("../../image/plan/selectCourse/ICN_Next_Disable_16x44.png") no-repeat;
}

.resultRight {
    background: url("../../image/plan/selectCourse/ICN_Next_16x44.png") no-repeat;
}

.resultMidContent {
	margin-left:4px;
	height:44px;
    width: 900px;
    display: inline-block;
    float: left;
    overflow: hidden;
}

#slideCourseResult {
	float:left;
	position:relative;
    width: auto;
    height: 44px;
    display: inline-block;
}

.selectCourseResult {
    position: absolute;
    bottom: 50px;
    top: 444px;
    
}

#selectCourseResultSpan {
	float: left;
	width: 960px;
	display: inline-block;
}

.courseResult {
	font-family: Arial Regular;
	font-size: 14pt;
	color: #418FB5;
	float: left;
	display: inline;
	margin-left: 10px;
	margin-top: 10px;
	padding-left: 5px;
	padding-right: 5px;
	background: #DDF1F8;
}

.removeCourse {
	width: 11px;
	height: 11px;
	margin-left: 5px;
	display: inline-block;
	background: url("../../image/buttonIMG/ICN_Close_11x11.png");
}

/*************************/
.selectedCourseDiv {
	position: absolute;
    width: 290px;
    padding-left: 5px;
    padding-right: 5px;
}

.selectedCourseLeft, .selectedCourseMid, .selectedCourseRight {
    float: left;
    height:20px;
    display:inline-block;
}
.selectedCourseLeft {
    width:5px;
    background: url("../../image/plan/selectCourse/IMG_Selected_Left_5x20.png")
		no-repeat;
}

.selectedCourseMid {
    width:280px;
    background: url("../../image/plan/selectCourse/IMG_Selected_Middle_1x20.png")
		repeat-x;
	color: #418FB5;
    font-size: 12px;
	font-style: Arial;
}

.selectedCourseRight {
    width:5px;
    background: url("../../image/plan/selectCourse/IMG_Selected_Right_5x20.png")
		no-repeat;
}

.idSpan, .nameSpan {
    float: left;
    line-height: 20px;
    height:20px;
}

.idSpan {
	padding-right: 10px;
}

.nameSpan {
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
    color:#418FB5;
    width:208px;
    display: inline-block;
}

.closeSpan {
	vertical-align:middle;
    height:10px;
    width:10px;
    float: right;
    margin-top: 5px;
    background: url("../../image/plan/selectCourse/ICN_Close_10x10.png")
		no-repeat;
}

.closeSpan:Hover {
    background: url("../../image/plan/selectCourse/ICN_Close_Hover_10x10.png")
		no-repeat;
}

/******* selected result part end*******/

#selectCourseList,#selectPlanList,#selectProgramList {
	display: none;
}

#midPartOfCourse,#midPartOfPlan,#midPartOfProgram {
	font-family: Arial;
	font-weight: Bold;
	font-size: 14px;
}

.selectCourseOperates {
    text-align: center;
	position: absolute;
	bottom: 10px;
	width: 960px;
}

#confirmButton {
	margin-right: 20px;
}

.dataList-a-pageSize {
    color: #555;
}

#searchConditionOfPlan {
	float: left;
	margin: 20px 0px 0px 10px
}

#keywordOfPlan {
	color: #555;
}
#searchButtonOfPlan {
	left: 50px;
	vertical-align: top;
}
#filterDivOfPlan{
	display: inline-block;
	vertical-align: middle;
    position:absolute;
    z-index: 1;
}

#filterBtnOfPlan{
    border: none;
    cursor:pointer;
    position:relative;
    z-index: 2;
}

#filterBoxOfPlan{
	display:none;
	position:absolute;
	float:left;
    top:31px;
    left:-350px;
	width:458px;
	padding:10px 20px;
	background-color: #F5F5F5;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	border: 1px solid #BFBFBF;
	z-index: 1;
}

#resetOfPlan{
    position: relative;
    float: right;
    margin-bottom:10px;
    font-family: arial;
    font-size: 14px;
    color: #418FB5;
}
#exsitedFlagOfPlan{
	display:none;
	width:16px;
	height:16px;
	position:absolute;
	left:20px;
	top:-3px;
	z-index:3;
	cursor:pointer;
    background: url("../../filterBox/images/IMG_Filtered_16x16.png") no-repeat;
}

.dataList-a-page {
    color: #555;
}

.courseNameValue {
    display: none;
    font-size: 12px;
}
