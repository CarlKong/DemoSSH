/* Example tokeninput style #2: tag style */
ul.token-input-list-tag {
    overflow-y: auto;
    overflow-x:hidden; 
    height: auto !important; 
    border: 1px solid #BFBFBF;
    cursor: text;
    font-size: 12px;
    font-family: Verdana;
    min-height: 1px;
    z-index: 999;
    margin: 0;
    padding: 0;
    background-color: #fff;
    list-style-type: none;
    clear: left;
    border-radius: 2px;
	box-shadow: inset 0 0 5px #BFBFBF;
}
 
ul.token-input-list-tag li input {
    border: 0;
    width: 100px;
    padding: 3px 8px;
    background-color: white;
    margin: 2px 0;
    -webkit-appearance: caret;
}
 
li.token-input-token-tag {
    overflow: hidden; 
    height: auto !important; 
    height: 15px;
    margin: 3px;
    padding: 1px 3px;
    background-color: #eff2f7;
    color: #000;
    cursor: default;
    border: 1px solid #ccd5e4;
    font-size: 11px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    float: left;
    white-space: nowrap;
}
 
li.token-input-token-tag p {
    display: inline;
    padding: 0;
    margin: 0;
}
 
li.token-input-token-tag span {
    color: #a6b3cf;
    margin-left: 5px;
    font-weight: bold;
    cursor: pointer;
}
 
li.token-input-selected-token-tag {
    background-color: #5670a6;
    border: 1px solid #3b5998;
    color: #fff;
}
 
li.token-input-input-token-tag {
    float: left;
    margin: 0;
    padding: 0;
    list-style-type: none;
}
